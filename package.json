{
  "name": "transfort",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "integration-test:e2e": "jest --config ./test/jest-e2e-integration.json",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig-migrations.ts",
    "migration-generate": "nest build && yarn run typeorm:cli  -- migration:generate -d ./migrations -n",
    "migration-run": "nest build && yarn run typeorm:cli  -- migration:run -d ./migrations -n",
    "migration-revert": "nest build && yarn run typeorm:cli  -- migration:revert"
  },
  "dependencies": {
    "@casl/ability": "^5.2.0",
    "@google-cloud/storage": "^5.8.5",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mapped-types": "^0.3.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/typeorm": "^7.1.5",
    "@types/json2csv": "^5.0.3",
    "@types/multer": "^1.4.5",
    "@types/objects-to-csv": "^1.3.1",
    "async-local-storage": "^2.3.1",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.6",
    "csvtojson": "^2.0.10",
    "express-basic-auth": "^1.2.0",
    "json2csv": "^5.0.6",
    "nest-access-control": "^2.0.2",
    "nestjs-pino": "^1.4.0",
    "otplib": "^12.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pino": "^6.13.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "schema-dts": "^0.8.2",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@trendyol/jest-testcontainers": "^2.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "testcontainers": "^7.20.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/test/utils/envVars.js"
    ]
  }
}
